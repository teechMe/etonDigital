{
  "devDependencies": {
    "@types/node": "^22.13.13",
    "cypress": "^14.1.0",
    "cypress-if": "^1.13.2",
    "cypress-junit-reporter": "^1.3.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-repeat": "^2.3.8",
    "cypress-xpath": "^2.0.1",
    "mocha": "^11.1.0",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "test": "cypress run --reporter mocha-junit-reporter --reporter-options mochaFile=cypress/results/junit/results.xml",
    "test:ci": "npm run test -- --headless --browser chrome",
    "test:dev": "cypress open",
    "test:html": "cypress run --reporter cypress-mochawesome-reporter",
    "test:full": "npm run test:ci && npm run test:html",
    "report": "npx mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochawesome/output.json && npx marge cypress/reports/mochawesome/output.json --reportDir cypress/reports/html --inline"
  }
}
